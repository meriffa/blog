{
  "Description": "Extract 'IPAddress' Objects",
  "Filter": "o => o.TypeName() == \u0022System.Net.IPAddress\u0022",
  "Fields": [
    {
      "Name": "Address",
      "Expression": "o => o.AddressHex()"
    },
    {
      "Name": "Type",
      "Expression": "o => o.TypeName()"
    },
    {
      "Name": "IPAddress",
      "Expression": "o => o.IPAddress()"
    }
  ]
}